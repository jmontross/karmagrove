<h1>Karma Grove emerging markets</h1>

<div class ="row">

   <div class="span12">

 <%=  image_tag("KarmicFlow.png", :class => "karmatree") %>
  </div>
</div>



<table>
  <tr>
    <th>State</th>
    <th>County</th>
    <th>District</th>
    <th>Intersection</th>
    <th>Address</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @markets.each do |market| %>
  <tr>
    <td><%= market.state %></td>
    <td><%= market.county %></td>
    <td><%= market.district %></td>
    <td><%= market.intersection %></td>
    <td><%= market.address %></td>
    <td><%= link_to 'Show', market %></td>
    <td><%= link_to 'Edit', edit_market_path(market) %></td>
    <td><%= link_to 'Destroy', market, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Market', new_market_path %>
